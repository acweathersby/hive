{
    "tasks": [
        {
            "type": "shell",
            "label": "clang++ build active file",
            "command": "/usr/local/clang_9.0.0/bin/clang++",
            "args": [
                "-g",
                "${file}",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}"
            ],
            "options": {
                "cwd": "/usr/local/clang_9.0.0/bin"
            }
        },
        {
            "type": "npm",
            "script": "build:dev",
            "problemMatcher": []
        }
    ],
    "version": "2.0.0"
}